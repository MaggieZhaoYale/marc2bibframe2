    <x:expect label="$6 222-xx should make a KeyTitle" test="//bf:Title[1]/rdf:type[2] = 'bf:KeyTitle'"/>
    <x:expect label="$6 242-xx should make a VariantTitle with varientType 'translated'" test="//bf:Instance[1]/bf:title[3]/bf:Title/bf:variantType = 'translated'"/>
    <x:expect label="$6 243-xx should make a CollectiveTitle" test="//bf:Work[1]/bf:title[1]/bf:Title/rdf:type[2] = 'bf:CollectiveTitle'"/>
